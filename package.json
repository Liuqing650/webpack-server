{
  "name": "webpack-server",
  "version": "0.0.5",
  "description": "webpack服务端项目",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liuqing650/webpack-server"
  },
  "homepage": "https://github.com/Liuqing650/webpack-server",
  "author": "liuqing",
  "license": "ISC",
  "main": "bin/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config webpack/dev.config.js",
    "server": "concurrently --kill-others \"better-npm-run watch-client\" \"better-npm-run start-dev\"",
    "build": "webpack --config webpack/prod.config.js --progress --color --profile",
    "start_prod": "concurrently --kill-others \"better-npm-run start-prod\"",
    "stop_prod": "pm2 delete webpack_server_prod"
  },
  "betterScripts": {
    "start-dev": {
      "command": "supervisor ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "supervisor webpack/wepack-dev-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "PORT": 3000
      }
    },
    "start-prod": {
      "command": "pm2 start ./bin/server.js --name=webpack_server_prod",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "HOST": "127.0.0.1",
        "PORT": 6000
      }
    }
  },
  "dependencies": {
    "mobx": "^3.0.0",
    "mobx-react": "^4.4.3",
    "mobx-react-router": "^3.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^3.2.1",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.3.1",
    "less-loader": "^3.0.0",
    "prop-types": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": ">=6.11.5"
  }
}
