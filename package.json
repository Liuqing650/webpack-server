{
  "name": "webpack-server",
  "version": "0.0.5",
  "description": "webpack服务端项目",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liuqing650/webpack-server"
  },
  "homepage": "https://github.com/Liuqing650/webpack-server",
  "author": "liuqing",
  "license": "ISC",
  "main": "bin/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dashboard yarn start",
    "start": "concurrently --kill-others \"better-npm-run start-dev\"",
    "build": "better-npm-run build",
    "start_prod": "concurrently --kill-others \"better-npm-run start-prod\"",
    "stop_prod": "pm2 delete webpack_server_prod"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "127.0.0.1",
        "PORT": 3300
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "pm2 start ./bin/server.js --name=webpack_server_prod",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "HOST": "127.0.0.1",
        "PORT": 6000
      }
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js",
      "src/helpers/Html.js"
    ]
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "compression": "^1.7.2",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "loadable-components": "^1.2.0",
    "mobx": "^3.0.0",
    "mobx-react": "^4.4.3",
    "mobx-react-router": "^4.0.2",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-loadable": "^5.3.1",
    "react-router": "3.2.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "asset-require-hook": "^1.2.0",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "css-modules-require-hook": "^4.2.3",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-minifier": "^3.5.15",
    "image-webpack-loader": "^4.2.0",
    "less": "^2.3.1",
    "less-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.3.0",
    "nodemon": "^1.17.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss": "^6.0.21",
    "postcss-less": "^1.1.5",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
